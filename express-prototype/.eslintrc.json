{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    // "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "no-trailing-spaces": "error",
        "semi": ["error", "always", { "omitLastInOneLineBlock": true}],
        "semi-spacing": "error",
        "semi-style": "error"
    }
}
